# Custom
font.sans-serif: Luxi Sans, Overpass, Helvetica, Helvetica Neue, Arial, Liberation Sans, DejaVu Sans, Bitstream Vera Sans, sans-serif
axes.prop_cycle: cycler('color', ["#1898e0", "#8bcd45", "#f89e13", "#d24493", "#00b2ed", "#dbe622", "#fb4c27", "#8f57bf", "#00bb62", "#f9c410", "#fb4865", "#645ccc"])
