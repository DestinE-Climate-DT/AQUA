# Default types

param:      Param;
step:       Step;
date:       Date;
hdate:      Date;
refdate:    Date;
latitude:   Double;
longitude:  Double;
levelist:   Double;
grid:       Grid;
expver:     Expver;

time:       Time;
fcmonth:    Integer;

number:     Integer;
frequency:  Integer;
direction:  Integer;
channel:    Integer;

instrument: Integer;
ident:      Integer;

diagnostic: Integer;
iteration:  Integer;
system:     Integer;
method:     Integer;

# pyfdb test data
[ class=ea, expver, stream, date, domain
       [ type, levtype
              [ time, step?, levelist?, param ]]]


# oper/dcda (modified, see last entry with type=ea)
[ class, expver, stream=oper/dcda/scda, date, time, domain?

       [ type=im/sim
               [ step?, ident, instrument, channel ]]

       [ type=ssd
               [ step, param, ident, instrument, channel ]]

       [ type=4i, levtype
               [ step, iteration, levelist, param ]]

       [ type=me, levtype
               [ step, number, levelist?, param ]]

       [ type=ef, levtype
               [ step, levelist?, param, channel? ]]

       [ type=ofb/mfb
               [ obsgroup, reportype ]]

       [ type=ea, levtype
               [ step, levelist?, param ]]
]
